{
  "name": "anae-backend",
  "version": "0.0.1",
  "description": "anae-backend",
  "main": "src/index.js",
  "scripts": {
    "start": "cross-env DEBUG=neo4j-graphql-js nodemon src/index.js --exec babel-node",
    "build": "babel src --out-dir dist",
    "start:prod": "pm2 start ecosystem.config.js --env production",
    "lint": "eslint ./src/**",
    "fix:lint": "eslint ./src/** --fix",
    "fix:yarn": "npm rebuild --update-binary",
    "test": "cross-env NODE_ENV=test nyc ava --verbose"
  },
  "repository": {
    "type": "git",
    "url": "git+ssh://git@bitbucket.org/izardc/anae-backend.git"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "homepage": "https://bitbucket.org/izardc/anae-backend#readme",
  "ava": {
    "files": [
      "tests/**/*.test.js"
    ],
    "require": [
      "@babel/register"
    ]
  },
  "nyc": {
    "require": [
      "@babel/register"
    ],
    "reporter": [
      "lcov",
      "text"
    ],
    "sourceMap": false,
    "instrument": false
  },
  "dependencies": {
    "@babel/runtime": "7.7.7",
    "apollo-server-express": "2.9.14",
    "argon2": "0.25.1",
    "chalk": "3.0.0",
    "cors": "^2.8.5",
    "cross-env": "^6.0.3",
    "dayjs": "1.8.18",
    "dotenv": "8.2.0",
    "express": "^4.17.1",
    "global": "^4.4.0",
    "graphql": "^14.5.6",
    "graphql-iso-date": "^3.6.1",
    "instagram-analytics": "^3.0.1",
    "jsonwebtoken": "^8.5.1",
    "mongoose": "5.8.1",
    "mongoose-int32": "^0.3.1",
    "morgan": "^1.9.1",
    "neo4j-driver": "^4.0.1",
    "neo4j-graphql-js": "^2.11.0",
    "request-ip": "^2.1.3"
  },
  "devDependencies": {
    "@babel/cli": "7.7.7",
    "@babel/core": "7.7.7",
    "@babel/node": "7.7.7",
    "@babel/plugin-proposal-object-rest-spread": "7.7.7",
    "@babel/plugin-transform-runtime": "7.7.6",
    "@babel/preset-env": "7.7.7",
    "@babel/preset-stage-2": "^7.0.0",
    "@babel/register": "7.7.7",
    "apollo-server-testing": "^2.9.15",
    "ava": "2.4.0",
    "babel-eslint": "^10.0.3",
    "babel-loader": "^8.0.6",
    "babel-plugin-istanbul": "^5.2.0",
    "casual": "^1.6.2",
    "eslint": "6.7.2",
    "eslint-config-airbnb-base": "^14.0.0",
    "eslint-plugin-import": "2.19.1",
    "nodemon": "2.0.2",
    "nyc": "^14.1.1",
    "pm2": "4.2.1"
  }
}
